{
  "name": "openwhisk-debugger",
  "version": "0.0.23",
  "description": "A debugger for OpenWhisk",
  "repository": {
    "type": "git",
    "url": "https://github.com/openwhisk/openwhisk-debugger.git"
  },
  "main": "client.js",
  "bin": {
    "wskdb": "./bin/wskdb"
  },
  "scripts": {
