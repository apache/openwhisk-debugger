  "dependencies": {
    "argv": "0.0.2",
    "colors": "^1.1.2",
    "columnify": "^1.5.4",
    "diff": "^3.0.1",
    "expand-home-dir": "0.0.3",
    "inquirer": "git://github.com/starpit/Inquirer.js.git",
    "node-inspector": "git://github.com/kerryspchang/node-inspector.git",
